<?xml version="1.0"?>
<transfers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="spryker:transfer-01"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Comment">
        <property name="latestVersion" type="CommentVersion" />
    </transfer>

    <transfer name="CommentResponse">
        <property name="comment" type="Comment" />
        <property name="isSuccessful" type="bool" />
        <property name="messages" type="Message[]"  singular="message" />
    </transfer>

    <transfer name="Quote">
        <property name="commentVersionReference" type="string" />
        <property name="commentReference" type="string" />
    </transfer>

    <transfer name="CompanyUser">
        <property name="idCompanyUser" type="int" />
    </transfer>

    <transfer name="Message">
        <property name="value" type="string"/>
    </transfer>
</transfers>
